{
	"name": "bricksPipe",
	"properties": {
		"description": "DataBricks Pipe",
		"activities": [
			{
				"name": "notebookArgs",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/ygutfreund@outlook.com/flightsADF",
					"baseParameters": {
						"input": {
							"value": "@pipeline().parameters.inputPath",
							"type": "Expression"
						},
						"output": {
							"value": "@pipeline().parameters.outputPath",
							"type": "Expression"
						},
						"pipelineRunId ": {
							"value": "@pipeline().RunId",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "ktg1bricks",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Validation1",
				"type": "Validation",
